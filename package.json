{
  "name": "ess-webpack-starter",
  "version": "1.0.0",
  "description": "A simple Webpack-based starter setup for front-end development.",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^3.4.0",
    "file-loader": "^6.2.0",
    "focus-visible": "^5.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^1.6.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^5.3.0",
    "prettier": "^2.3.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "terser-webpack-plugin": "^5.1.2",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "scripts": {
    "serve": "npx serve . -l tcp://127.0.0.1:8080",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "watch:assets": "cross-env NODE_ENV=development webpack --config webpack.config.js -w",
    "watch:browser-sync": "browser-sync start --config browsersync.config.js",
    "watch": "run-p watch:*",
    "prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "lint": "eslint assets/js/site.js"
  }
}
