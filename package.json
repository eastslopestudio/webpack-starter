{
  "name": "ess-webpack-starter",
  "version": "1.0.0",
  "description": "A simple Webpack-based starter setup for front-end development.",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.12",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.1.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.11.2",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.2",
    "prettier": "^2.1.2",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "terser-webpack-plugin": "^4.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "focus-visible": "^5.1.0",
    "tailwindcss": "^1.8.10"
  },
  "scripts": {
    "serve": "npx serve . -l tcp://127.0.0.1:8080",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "watch:assets": "cross-env NODE_ENV=development webpack --config webpack.config.js -w",
    "watch:browser-sync": "browser-sync start --config browsersync.config.js",
    "watch": "run-p watch:*",
    "prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "lint": "eslint assets/js/site.js"
  }
}
